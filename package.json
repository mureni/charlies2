{
   "name": "charlies",
   "version": "2.2.0",
   "main": "./dist/index.js",
   "author": "mureni <mureni.leijona@gmail.com>",
   "license": "MIT",
   "files": [
      "./dist",
      "./resources"
   ],
   "dependencies": {
      "app-root-path": "3.1.0",
      "better-sqlite3": "12.6.2",
      "canvas": "3.2.0",      
      "discord.js": "14.25.1",
      "dotenv": "17.2.3",
      "rimraf": "^6.1.2",
      "source-map-support": "0.5.21",
      "tsconfig-paths": "4.2.0",
      "winston": "3.19.0",
      "winston-daily-rotate-file": "5.0.0"
   },
   "overrides": {
      "undici": "^6.23.0"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "8.54.0",
      "@typescript-eslint/parser": "8.54.0",
      "@types/better-sqlite3": "7.6.13",      
      "@types/node": "25.1.0",
      "@types/ws": "8.18.1",
      "eslint": "9.39.2",
      "ts-node": "10.9.2",
      "typescript": "5.9.3",
      "vitest": "4.0.18"
   },
  "scripts": {
      "clean": "rimraf dist",
      "prebuild": "rimraf dist",
      "build": "tsc",
      "build:watch": "tsc --watch",
      "dev": "npm run build && npm run start",
      "admin:portal": "ts-node --transpile-only src/admin-portal/index.ts",
      "start": "node ./dist/index.js",
      "test": "vitest",
      "test:run": "vitest run",
      "test:types": "tsc -p tsconfig.test.json --noEmit",
      "test:sqlite": "ts-node -r tsconfig-paths/register --transpile-only tests/sqlite-collections.test.ts",
      "lint": "eslint \"{src,tests,tools}/**/*.ts\"",
    "test:sqlite:check": "tsc -p tsconfig.sqlite-test.json --noEmit"
  },
  "engines": {
    "node": ">=24"
  },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/mureni/charlies2.git"
   }
}
