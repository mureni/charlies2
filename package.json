      "test:types": "tsc -p tsconfig.test.json --noEmit",
      "test:sqlite": "ts-node --transpile-only tests/sqlite-collections.test.ts",
      "test:sqlite:check": "tsc -p tests/tsconfig.sqlite-test.json --noEmit"
