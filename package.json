   "files": [
      "./dist",
      "./resources"
   ],
   "dependencies": {
      "app-root-path": "3.0.0",
      "better-sqlite3": "12.5.0",
      "canvas": "3.2.0",
      "cli-progress": "3.10.0",
      "discord.js": "14.25.1",
      "dotenv": "17.2.3",
      "rimraf": "^6.1.2",
      "source-map-support": "0.5.21",
      "winston": "3.19.0",
      "winston-daily-rotate-file": "5.0.0"
   },
   "devDependencies": {
      "@types/better-sqlite3": "7.6.13",
      "@types/cli-progress": "3.9.2",
      "@types/node": "25.0.3",
      "@types/ws": "8.18.1",
      "ts-node": "10.9.2",
      "typescript": "5.9.3",
      "vitest": "3.2.4"
   },
   "scripts": {
      "clean": "rimraf dist",
      "prebuild": "rimraf dist",
      "build": "tsc",
      "build:watch": "tsc --watch",
